{
  "name": "3dwebsite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npx kill-port 4200 && ng serve --host 0.0.0.0 --port 4200",
    "build": "ng build && node ./cloudflare-postbuild.mjs",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr": "node dist/3dwebsite/server/server.mjs",
    "dev:ssr": "ng run 3dwebsite:serve-ssr",
    "build:ssr": "ng build && ng run 3dwebsite:server",
    "prerender": "ng run 3dwebsite:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/platform-server": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@angular/ssr": "^19.1.6",
    "angular-three": "^3.7.2",
    "angular-three-soba": "^3.7.2",
    "camera-controls": "^2.10.0",
    "express": "^4.18.2",
    "maath": "^0.10.8",
    "ngx-extended-pdf-viewer": "^25.6.4",
    "ngxtension": "^4.0.0",
    "rxjs": "~7.8.0",
    "three": "^0.171.0",
    "three-stdlib": "^2.35.13",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
